<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Encrypted</os:ShortName>
<os:Description>Google Encrypted</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://encrypted.google.com/</SearchForm>
<os:Url type="text/html" method="GET" template="https://encrypted.google.com/search">
  <os:Param name="hl" value="en"/>
  <os:Param name="tbo" value="d"/>
  <os:Param name="output" value="search"/>
  <os:Param name="sclient" value="psy-ab"/>
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="btnG" value=""/>
  <os:Param name="oq" value=""/>
  <os:Param name="gs_l" value=""/>
  <os:Param name="pbx" value="1"/>
</os:Url>
</SearchPlugin>